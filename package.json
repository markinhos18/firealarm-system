{
  "name": "onfire",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "db": "json-server --watch ./api/searchmock/products.json",
    "dev:db": "concurrently \"yarn dev\" \"yarn db\""
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@styled-icons/bootstrap": "^10.47.0",
    "@styled-icons/boxicons-regular": "^10.47.0",
    "@styled-icons/boxicons-solid": "^10.47.0",
    "@styled-icons/fluentui-system-regular": "^10.47.0",
    "@styled-icons/heroicons-outline": "^10.47.0",
    "@styled-icons/material-outlined": "^10.47.0",
    "@styled-icons/remix-fill": "^10.46.0",
    "next": "12.0.10",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-charts": "^4.0.0",
    "styled-components": "5.3.3",
    "styled-icons": "^10.47.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.14.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/json-server": "^0.14.4",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "json-server": "^0.17.1",
    "lint-staged": "^12.3.3",
    "plop": "^3.0.5",
    "polished": "^4.2.2",
    "prettier": "^2.4.1",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  }
}
